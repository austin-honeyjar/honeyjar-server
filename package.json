{
  "name": "honeyjar-server",
  "version": "1.0.0",
  "description": "API server for Honeyjar application",
  "main": "dist/server.ts",
  "type": "module",
  "scripts": {
    "start": "node dist/server.ts",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:workflow": "tsx unit-test/workflow.test.ts",
    "test:base-workflow": "tsx unit-test/base-workflow.test.ts",
    "test:base": "tsx unit-test/base.test.ts",
    "test:auth": "tsx unit-test/auth.test.ts"
  },
  "dependencies": {
    "@clerk/backend": "^1.27.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "openai": "^4.95.0",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.42.0",
    "jest": "^29.7.0",
    "postgres": "^3.4.5",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
